{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "prague-clock": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "cs": {
            "translation": "src/locales/messages.cs.xlf"
          },
          "de": {
            "translation": "src/locales/messages.de.xlf"
          },
          "es": {
            "translation": "src/locales/messages.es.xlf"
          },
          "fr": {
            "translation": "src/locales/messages.fr.xlf"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "localize": true,
            "outputPath": "dist/prague-clock",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/primeng/resources/themes/arya-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/@tubular/ng-widgets/assets/styles.css",
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": {
                "scripts": true,
                "fonts": true,
                "styles": false
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "localize": ["en-US"]
            },
            "cs": {
              "localize": ["cs"]
            },
            "de": {
              "localize": ["de"]
            },
            "es": {
              "localize": ["es"]
            },
            "fr": {
              "localize": ["fr"]
            },
            "en-US": {
              "localize": ["en-US"]
            }
         },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "prague-clock:build:production"
            },
            "development": {
              "browserTarget": "prague-clock:build:development"
            },
            "cs": {
              "browserTarget": "prague-clock:build:cs"
            },
            "de": {
              "browserTarget": "prague-clock:build:de"
            },
            "es": {
              "browserTarget": "prague-clock:build:es"
            },
            "fr": {
              "browserTarget": "prague-clock:build:fr"
            },
            "en-US": {
              "browserTarget": "prague-clock:build:en-US"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "prague-clock:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "prague-clock"
}
